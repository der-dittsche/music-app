{
  "name": "music-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "devfb": "firebase emulators:start --import=./saved-data --export-on-exit",
    "fbexport": "firebase emulators:export ./saved-data",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --ui",
